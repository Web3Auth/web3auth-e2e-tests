{
  "files.trimTrailingWhitespace": true,
  "files.trimFinalNewlines": true,
  "files.insertFinalNewline": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "files.exclude": {
    "node_modules": true,
    "yarn.lock": true
  },
  "[markdown]": {
    "editor.wordWrap": "off"
  },
  "cSpell.words": [
    "chetan",
    "creds",
    "deps",
    "fjogeleit",
    "gmail",
    "isrefinement",
    "jakejarvis",
    "Mailosaur",
    "mkdir",
    "networkidle",
    "noreport",
    "openlogin",
    "passwordless",
    "tkey",
    "toruslabs"
  ]
}
