{
  "name": "@toruslabs/e2e-tests",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "test:browser": "playwright test --headed",
    "test": "playwright test",
    "test:ci": "./bin/runTestsCI.sh",
    "test:trace": "playwright test --trace retain-on-failure --reporter=html",
    "test:config": "playwright test --workers=1 --trace retain-on-failure --reporter=html --config=index.config.ts",
    "trace:show": "playwright show-trace",
    "install-deps": "playwright install-deps",
    "docker:build": "docker build -t test-image .",
    "docker:run": "docker run -it test-image /bin/bash"
  },
  "dependencies": {
    "@playwright/test": "^1.32.3",
    "bip39": "^3.0.4",
    "dotenv": "^16.0.3",
    "generate-password": "^1.7.0",
    "mailosaur": "^8.5.0",
    "playwright": "^1.30.0",
    "serve-handler": "^6.1.3"
  },
  "devDependencies": {
    "@tsconfig/node16": "^1.0.3",
    "@types/node": "^16",
    "@types/serve-handler": "^6.1.1",
    "lighthouse": "^9.6.3",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  }
}
